# Read up to 500 objects.
AwsS3(S3Command="ListBucketObjects",Region="us-west-2",Bucket="software.openwaterfoundation.org",MaxKeys=500,Prefix=cdss-app-snodas-tools-doc-dev,OutputTableID="BucketObjectsUsingPrefix")
# Only read keys starting with the prefix, including a slash.
AwsS3(S3Command="ListBucketObjects",Region="us-west-2",Bucket="software.openwaterfoundation.org",MaxKeys=500,Prefix=cdss-app-snodas-tools-doc-dev/software-design,OutputTableID="BucketObjectsUsingLongPrefix")
# Try to read all objects in a bucket up to to the maximum limit.
AwsS3(S3Command="ListBucketObjects",Region="us-west-2",Bucket="software.openwaterfoundation.org",MaxObjects=8000,OutputTableID="BucketObjectsDefaultMaxObjects")
# Copy an object on the test bucket.
AwsS3(S3Command="CopyObject",Region="us-west-2",Bucket="owf-test",CopySourceKey="index.html",CopyDestKey="test-index.html")
# Delete an object on the test bucket.
AwsS3(S3Command="DeleteObject",Region="us-west-2",Bucket="owf-test",DeleteKey="test-index.html")
